<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
         xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:menu="urn:import:org.insset.client.menu">

  <g:HTMLPanel>
    <menu:Menu ui:field="menu"/>
    <h1>Projet de mise en pratique Scrum</h1>       
    <div class="bs-section">

        <g:FlowPanel>  
            <g:FormPanel/>
              
            <g:FlowPanel>
                <g:Label>Division</g:Label>
            </g:FlowPanel>
            <g:FlowPanel>
                <g:Label>Valeur1</g:Label>
            </g:FlowPanel>
            <g:FlowPanel>          
              <g:TextBox ui:field="valA" text="Entrer un nombre"/>
            </g:FlowPanel>
            <g:FlowPanel>
                <g:Label>Valeur2</g:Label>
            </g:FlowPanel>
            <g:FlowPanel>          
              <g:TextBox ui:field="valB" text="Entrer un nombre"/>
            </g:FlowPanel>
            <g:FlowPanel ui:field="formActions" >
                <g:SubmitButton ui:field="boutonDiv" text="Envoyer"/>
                <g:ResetButton ui:field="boutonClear" text="Clear"/>
            </g:FlowPanel>
            <g:FlowPanel>
                <g:Label ui:field="errorLabelRToA" ></g:Label>
            </g:FlowPanel>
            
            <g:FlowPanel>
                <g:Label>Remise sur prix</g:Label>
            </g:FlowPanel>
            <g:FlowPanel>          
              <g:TextBox ui:field="porce" text="Entrer un nombre"/>
            </g:FlowPanel>
            <g:FlowPanel>
                <g:Label ui:field="errorLabel" ></g:Label>
            </g:FlowPanel>
            <g:FlowPanel>
                <g:SubmitButton ui:field="boutonEnregistrer" text="Envoyer"/>
                <g:ResetButton ui:field="boutonClearB" text="Clear"/>
            </g:FlowPanel>
        </g:FlowPanel>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>
